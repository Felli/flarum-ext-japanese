{
    "name": "felli/lang-japanese",
    "type": "flarum-extension",
    "description": "Japanese language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": {
         "name": "Sarah Tsumayoi",
         "email": "tsuharusarah873@gmail.com"
    },
    "support": {
         "issues": "https://github.com/felli/lang-japanese/issues",
         "source": "https://github.com/felli/lang-japanese"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Japanese",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "ja_jp",
            "title": "日本語"
        }
    }
}
